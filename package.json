{
	"name": "agendaai",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"dev:front": "next dev --turbopack",
		"dev:api": "npm --prefix api run dev",
		"dev:full": "concurrently -k -n front,api -c cyan,magenta \"npm run dev:front\" \"npm run dev:api\"",
		"build": "next build --turbopack",
		"start": "next start",
		"start:api": "npm --prefix api run start",
		"start:full": "concurrently -k -n front,api -c cyan,magenta \"npm run start\" \"npm run start:api\"",
		"lint": "eslint"
	},
	"dependencies": {
		"lucide-react": "^0.546.0",
		"next": "15.5.9",
		"react": "19.1.0",
		"react-dom": "19.1.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.1.2",
		"eslint": "^9",
		"eslint-config-next": "15.5.4",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.18",
		"typescript": "^5"
	},
	"overrides": {
		"glob": "^10.5.0",
		"js-yaml": "^4.1.1"
	}
}
